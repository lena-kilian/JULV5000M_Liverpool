    <script type="text/javascript">
        function getPrefFntSz() { return $.cookie('preffontsize') || 'l'; }
        function setPrefFntSz(sz) { $.cookie('preffontsize', sz, { expires: 600, path: '/' }); updUiPrefFnt(sz); document.body.style.fontSize = getPrefFntSzByAlias(sz); ga('send', 'event', 'Miscellaneous', 'Change text size', sz); }
        function getPrefFntSzByAlias(sz) { var sizes = { xs: '10px', s: '11px', m: '12px', l: '13px', xl: '15px' }; return sizes[sz] || sizes.m; }
        function updUiPrefFnt(sz) { $('.prefFntCtrl').removeClass('activeFntSz'); $('#prefFntBtn_' + sz).addClass('activeFntSz'); }
        function applyPrefFntSz(sz) { document.write('<style>body { font-size: ' + getPrefFntSzByAlias(sz) + '; }' + ((sz == 'xs' || sz == 's' || sz == 'm')? ' li { padding-bottom: 0px; }' : '') + '</style>'); $(document).ready(function () { updUiPrefFnt(sz); }); }
        applyPrefFntSz(getPrefFntSz()); 

        // Include D3 for browsers that support it
        if (Modernizr.inlinesvg) { document.write ('<script type="text\/javascript" src="\/js\/d3.v3.min.js"><\/script>'); }
    </script>
        
